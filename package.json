{
	"name": "@ctx-core/uri",
	"version": "11.0.0",
	"description": "ctx-core uri",
	"keywords": [
		"ctx-core",
		"uri"
	],
	"homepage": "https://github.com/ctx-core/uri#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/uri/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/uri/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./src/index.js",
	"module": "./src/index.js",
	"types": "./src/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.js && rm -f dist/**/*.d.ts && rm -f dist/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run compile"
	},
	"dependencies": {
		"@ctx-core/object": "workspace:^20.0.0"
	},
	"devDependencies": {
		"ava": "^3.15.0",
		"esm": "^3.2.25",
		"typescript": "^4.3.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"ava": {
		"require": [
			"esm"
		]
	}
}
