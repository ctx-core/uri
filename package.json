{
	"name": "@ctx-core/uri",
	"version": "12.0.6",
	"description": "ctx-core uri",
	"keywords": [
		"ctx-core",
		"uri"
	],
	"homepage": "https://github.com/ctx-core/uri#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/uri/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/uri/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"require": "./dist/index.cjs",
		"import": "./dist/index.js"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"require = require('esm')(module)\nmodule.exports = require('./index.js')\" > index.cjs",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/object": "workspace:^21.0.7"
	},
	"devDependencies": {
		"ava": "^3.15.0",
		"esm": "^3.2.25",
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"ava": {
		"require": [
			"esm"
		]
	}
}
