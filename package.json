{
	"name": "@ctx-core/uri",
	"version": "12.0.17",
	"description": "ctx-core uri",
	"keywords": [
		"ctx-core",
		"uri"
	],
	"homepage": "https://github.com/ctx-core/uri#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/uri/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/uri/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/object": "workspace:^21.0.23"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.51",
		"@swc/core": "^1.2.108",
		"ava": "^3.15.0",
		"typescript": "^4.4.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"ava": {
		"require": []
	}
}